{
    "version": "0.1.0",
    "name": "aiida_quantumespresso_uscf",
    "url": "https://github.com/sphuber/aiida-quantumespresso-uscf",
    "license": "MIT License",
    "author": "Sebastiaan P. Huber",
    "author_email": "mail@sphuber.net",
    "description": "The AiiDA plugin for the Uscf module of Quantum ESPRESSO",
    "classifiers": [
        "License :: OSI Approved :: MIT License",
        "Programming Language :: Python :: 2.7",
        "Development Status :: 4 - Beta"
    ],
    "install_requires": [
        "aiida",
        "aiida-quantumespresso"
    ],
    "entry_points": {
        "aiida.calculations": [
            "quantumespresso.uscf = aiida_quantumespresso_uscf.calculations.uscf:UscfCalculation"
        ],
        "aiida.parsers": [
            "quantumespresso.uscf = aiida_quantumespresso_uscf.parsers.uscf:UscfParser"
        ],
        "aiida.workflows": [
            "quantumespresso.uscf.main = aiida_quantumespresso_uscf.workflows.uscf.main:UscfWorkChain",
            "quantumespresso.uscf.parallelize_atoms = aiida_quantumespresso_uscf.workflows.uscf.parallelize_atoms:UscfParallelizeAtomsWorkChain",
            "quantumespresso.uscf.base = aiida_quantumespresso_uscf.workflows.uscf.base:UscfBaseWorkChain"
        ]
    }
}